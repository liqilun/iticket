<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.iticket.model.stadium.Stadium" table="stadium" lazy="false">
		<id name="id" type="long" column="recordid">
			<generator class="native" />
		</id>
		<property name="cnName" type="string" column="cn_name"/>
		<property name="enName" type="string" column="en_name"/>
		<property name="cnAddress" type="string" column="cn_address"/>
		<property name="contact" type="string" column="contact"/>
		<property name="telephone" type="string" column="telephone"/>
		<property name="status" type="string" column="status"/>
		<property name="refundPass" type="string" column="refund_pass"/>
		<property name="reprintPass" type="string" column="reprint_pass"/>
		<property name="orderNo" type="integer" column="order_no"/>
		<property name="addTime" type="timestamp" column="add_time"/>
	</class>
</hibernate-mapping>
